{
  "name": "raft-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --force",
    "start": "vite --host",
    "build-production": "tsc && vite build --mode production",
    "build-stage-goerli": "tsc && vite build --mode stage-goerli",
    "build-stage-mainnet": "tsc && vite build --mode stage-mainnet",
    "deploy-production": "firebase deploy --only hosting:raft-fi-app",
    "deploy-stage-goerli": "firebase deploy --only hosting:raft-fi-app-stage",
    "deploy-stage-mainnet": "firebase deploy --only hosting:raft-fi-app-mainnet",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "i18next-parser": "i18next --config src/i18n/config.ts",
    "pseudo-translation": "./scripts/pseudo-translation.sh"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.1",
    "@raft-fi/sdk": "^0.4.7",
    "@react-rxjs/core": "^0.10.4",
    "@react-rxjs/utils": "^0.9.5",
    "@tempusfinance/decimal": "^0.0.12",
    "@web3-onboard/common": "^2.3.1",
    "@web3-onboard/injected-wallets": "^2.8.5",
    "@web3-onboard/ledger": "^2.4.4",
    "@web3-onboard/react": "^2.8.0",
    "@web3-onboard/walletconnect": "^2.3.6",
    "axios": "^1.3.6",
    "date-fns": "^2.30.0",
    "ethers": "^6.3.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "is-mobile": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.3",
    "sass": "^1.62.0",
    "tempus-ui": "github:tempusfinance/tempus-ui#d49026d2d8ed88e4dbc4ef9a1446c551ecd69086",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "assert": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "i18next-parser": "^7.7.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-node-stdlib-browser": "^0.2.1"
  }
}
